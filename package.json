{
  "name": "beats-playlist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run createColors && vite",
    "devNoUnraid": "bun run createColors && NO_UNRAID=true vite",
    "build": "bun run createColors && tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "createColors": "bun run createTailwindColors.ts 2> /dev/null",
    "buildDocker": "docker build -t qodesmith/beats-playlist:1.0.0 --platform=linux/amd64 .",
    "pushDocker": "docker push qodesmith/beats-playlist:1.0.0",
    "deploy": "bun run build && bun run buildDocker && bun run pushDocker"
  },
  "dependencies": {
    "@elysiajs/server-timing": "^1.0.2",
    "elysia": "^1.0.27"
  },
  "devDependencies": {
    "@qodestack/dl-yt-playlist": "^7.4.0 ",
    "@qodestack/prettier-config": "^1.0.2",
    "@qodestack/utils": "^2.3.1",
    "@types/bun": "^1.1.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "framer-motion": "^11.3.8",
    "jotai": "^2.8.4",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
